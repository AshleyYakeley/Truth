let

# Define a type of entity for people
entity Person;

# Properties of Person
name = property @Person @Text !498260df-6a8a-44f0-b285-68a63565a33b;
mother = property @Person @Person !3afce58f-b7eb-4b11-8a75-2d66afd4d085;
father = property @Person @Person !c005705f-9259-4d24-9713-db28a6e4f7d5;

# Root type and point
entity Root;
root = point @Root !78baed51-cb05-46b5-bcb4-49031532b890;

# The set of people connected to root.
people = property @Person @Root !f06efa5e-190f-4e5d-8633-495c5683c124 !@ {root};

in
with (members (rev alphabetical) (name !$$ people)) $ \l -> for l outputln
