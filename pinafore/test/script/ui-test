#!/usr/bin/pinafore
let

emptywindow :: Action ();
emptywindow = do
    openWindow (300,400) {"Empty"} {[]} uiBlank;
    return ();
    end;

newpoint :: Action ();
newpoint = do s <- newMemFiniteSet; newEntity s; return (); end;

in openWindow (300,400) {"Test"} {[]} $ uiButton {"Button"} {newpoint >> emptywindow};
