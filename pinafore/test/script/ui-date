#!/usr/bin/pinafore
do
dref <- newMemWhole;
tref <- newMemWhole;
iref <- newMemWhole;
openWindow (300,400) {"Date"} {[]} $ uiVertical
    [
        (uiTextEntry tref, False),
        (uiTextEntry tref, False),
        (uiTextEntry $ interpretDateAsText dref, False),
        (uiTextEntry $ interpretDateAsText dref, False),
        (uiTextEntry $ interpretIntegerAsText iref, False),
        (uiTextEntry $ interpretIntegerAsText iref, False)
    ];
end
