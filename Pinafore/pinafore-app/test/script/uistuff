#!/usr/bin/pinafore
import "gnome", "UILib"
run.GTK $ fn gtk =>
new.UndoHandler >>= fn undoHander =>
imply {
    ?gtk = gtk;
    ?undoHandler = undoHander;
}
with Widget.GTK., UILib.
do {
    tref1 <- newMem.TextModel;
    tref2 <- newMem.TextModel;
    paneWindow.Pane $ simple.Pane "Text" $ vertical [layoutGrow $ textView tref1, layoutGrow $ textView tref2];
}
