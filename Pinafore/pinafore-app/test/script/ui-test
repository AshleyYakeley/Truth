#!/usr/bin/pinafore
let

emptywindow :: Action Unit;
emptywindow = do
    openWindow.GTK (300,400) {"Empty"} blank.GTK;
    return ();
    end;

newpoint :: Action Unit;
newpoint = do s <- newMemFiniteSetModel; newEntity s; return (); end;

in openWindow.GTK (300,400) {"Test"} $ button.GTK {"Button"} {newpoint >> emptywindow};
