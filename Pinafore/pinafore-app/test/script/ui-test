#!/usr/bin/pinafore
let

emptywindow :: Action Unit;
emptywindow = do
    GTK.openWindow (300,400) {"Empty"} GTK.blank;
    return ();
    end;

newpoint :: Action Unit;
newpoint = do s <- newMemFiniteSetModel; newEntity s; return (); end;

in GTK.openWindow (300,400) {"Test"} $ GTK.button {"Button"} {newpoint >> emptywindow};
