#!/usr/bin/pinafore
let

emptywindow :: Action Unit;
emptywindow = do
    open.Window.GTK (300,400) {"Empty"} blank.GTK;
    return ();
    end;

newpoint :: Action Unit;
newpoint = do s <- newMem.FiniteSetModel; newEntity s; return (); end;

in open.Window.GTK (300,400) {"Test"} $ button.GTK {"Button"} {newpoint >> emptywindow};
