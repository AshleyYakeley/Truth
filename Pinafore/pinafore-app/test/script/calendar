#!/usr/bin/pinafore
Env.openDefaultStore >>= fn store =>
let
import Storage;
pdate = property @Unit @Date !"calendar-test" store;
rdate = pdate !$ {()};
import GTK();
in GTK.run $ fn gtk => GTK.openWindow gtk (300,400) {"Test Calendar"} $ GTK.vertical [GTK.calendar rdate,GTK.layoutGrow $ GTK.label {show %rdate}]
