#!/usr/bin/pinafore
openDefaultStore.Env >>= fn store =>
let

pdate = property @Unit @Date !"calendar-test" store;
rdate = pdate !$ {()};
in
import "pinafore-gnome" in
run.GTK $ fn gtk => open.Window.GTK gtk (300,400) {"Test Calendar"} $ vertical.GTK [calendar.GTK rdate,layoutGrow.GTK $ label.GTK {show %rdate}]
