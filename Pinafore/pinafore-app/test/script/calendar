#!/usr/bin/pinafore
openDefaultStore.Env >>= fn store =>
let

pdate = property @Unit @Date !"calendar-test" store;
rdate = pdate !$ {()};
in
import "pinafore-gnome" in
with GTK. in
run $ fn gtk => open.Window gtk (300,400) {"Test Calendar"} $ vertical.Element [calendar.Element rdate,layoutGrow.Element $ label.Element {show %rdate}]
