#!/usr/bin/pinafore
do
dref <- newMemWholeModel;
tref <- newMemWholeModel;
iref <- newMemWholeModel;
openWindow.GTK (300,400) {"Date"} $ vertical.GTK
    [
        (textEntry.GTK tref, False),
        (textEntry.GTK tref, False),
        (textEntry.GTK $ interpretDateAsText dref, False),
        (textEntry.GTK $ interpretDateAsText dref, False),
        (textEntry.GTK $ interpretIntegerAsText iref, False),
        (textEntry.GTK $ interpretIntegerAsText iref, False)
    ];
end
