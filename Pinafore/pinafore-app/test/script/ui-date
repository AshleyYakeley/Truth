#!/usr/bin/pinafore
do
dref <- newMemWhole;
tref <- newMemWhole;
iref <- newMemWhole;
GTK.openWindow (300,400) {"Date"} $ GTK.vertical
    [
        (GTK.textEntry tref, False),
        (GTK.textEntry tref, False),
        (GTK.textEntry $ interpretDateAsText dref, False),
        (GTK.textEntry $ interpretDateAsText dref, False),
        (GTK.textEntry $ interpretIntegerAsText iref, False),
        (GTK.textEntry $ interpretIntegerAsText iref, False)
    ];
end
