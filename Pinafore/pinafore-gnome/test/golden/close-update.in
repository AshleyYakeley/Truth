#!/usr/bin/env -S pinafore
import "pinafore-gnome" in
with GTK. in
run.GTK $ fn gtk =>
let

    eui: Widget
    = exec.Widget $ do
        sleep $ Seconds 0.2;
        pure.Action blank.Widget;
        end;

in do
    model <- newMem.WholeModel;
    w <- open.Window gtk (600,800) {"Window"} $ dynamic.Widget $ coMap.WholeModel (fn _ => eui) model;
    model := ();
    sleep $ Seconds 0.1;
    close.Window w;
    end
