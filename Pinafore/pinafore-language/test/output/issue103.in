#!/usr/bin/pinafore

let

e1: Entity
= ();

f: WholeModel a -> Unit
= fn _ => ();

using Action (>>=,>>,return);
using WholeModel (get,:=);

in do
    (model,_) <- newMem.WholeModel >>= fn model => return (model, f model);
    model := e1;
    e2 <- get model;
    checkEntity.Debug "CHECK e1" e1;
    checkEntity.Debug "CHECK e2" e2;
    outputLn.Env "LAST";
    end
