#!/usr/bin/pinafore
let using Function($); using Lifecycle(onClose,closer); using Env in
do
    outputLn "A";
    onClose $ outputLn "B";
    outputLn "C";
    run.Lifecycle $ do
        outputLn "1";
        onClose $ outputLn "2";
        outputLn "3";
        end;
    outputLn "D";
    (_,cl) <- closer $ do
        outputLn "p";
        onClose $ outputLn "q";
        outputLn "r";
        end;
    outputLn "E";
    cl;
    outputLn "F";
    cl;
    outputLn "G";
    onClose $ do
        outputLn "x";
        onClose $ outputLn "y";
        outputLn "z";
        end;
    outputLn "H";
end
