#!/usr/bin/pinafore
let
using Function($,>-);
using Showable;
using Number;
using Integer;
using Action(for_);

fix: (a -> a) -> a
= fn f => let rec x = f x end in x;

rrange
= fns s a b => if a > b then [] else a :: s (succ a) b;

range: Integer -> Number -> List Integer
= fix rrange;
in for_ (range 1 5) $ fn n => outputLn.Env (show n)
